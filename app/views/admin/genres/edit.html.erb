<div class="col-10 mt-5">
  <h5>ジャンル編集</h5>
     <%= form_with model: @genre, url: admin_genre_path, class: "form-inline mt-4", local:true do |f| %>
       <div class="form-group">
         <label for="exampleInputTitle" class="mr-3">ジャンル名</label>
           <%= f.text_field :genre %>
       </div>
           <%= f.submit "変更を保存", class: "btn btn-success ml-3" %>
     <% end %>
</div>